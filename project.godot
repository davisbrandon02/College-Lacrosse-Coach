[application]

config/name="College Lacrosse Coach 2022"
run/main_scene="res://ui/MainMenu.tscn"

[autoload]

nameData="*res://data/nameData.gd"
career="*res://scripts/career_scripts/career.gd"
DB="*res://data/DB.gd"

[display]

window/size/width=720
window/size/height=1280
window/size/resizable=false
window/size/test_width=720
window/size/test_height=1280
window/handheld/orientation="portrait"
window/stretch/mode="2d"
